{
  "name": "hunspell-native",
  "version": "3.0.6",
  "description": "Cross-platform Node.js binding for Hunspell, the spellcheck library used by LibreOffice, Firefox, Chrome and Adobe.",
  "author": "Vincent Weevers",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "types": "src/index.d.ts",
  "files": [
    "patches",
    "scripts",
    "src",
    "!src/hunspell/msvc/",
    "!src/hunspell/tests/",
    "!src/hunspell/.travis.yml",
    "!src/hunspell/appveyor.yml",
    "binding.gyp",
    "CHANGELOG.md",
    "index.js"
  ],
  "dependencies": {
    "node-addon-api": "8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "neostandard": "^0.12.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "node-gyp rebuild -j 8",
    "build-debug": "node-gyp rebuild -j 8 --debug",
    "test": "eslint && node test/index.js",
    "test-watch": "node --test --watch test/index.js"
  },
  "engines": {
    "node": ">=22"
  },
  "contributors": [
    "Nathan Sweet <nathanjsweet@gmail.com>",
    "Haris Khan <hk12@live.ca>",
    "Titus Wormer <tituswormer@gmail.com>",
    "Espen Hovlandsdal <espen@hovlandsdal.com>",
    "Thomas Beverley <tom.beverley101@gmail.com>",
    "Bjarke Bech <bjarkebech@gmail.com>"
  ],
  "repository": "https://github.com/vweevers/hunspell-native",
  "bugs": "https://github.com/vweevers/hunspell-native/issues",
  "gypfile": true,
  "keywords": [
    "spell",
    "spelling",
    "spellcheck",
    "spellchecker",
    "hunspell"
  ]
}
