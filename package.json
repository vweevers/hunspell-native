{
  "name": "nodehun-native",
  "version": "3.0.6",
  "description": "The Hunspell binding for nodejs that exposes as much of hunspell as possible and also adds new features. Hunspell is a first class spellcheck library used by Google, Apple, and Mozilla.",
  "license": "MIT",
  "keywords": [
    "spell",
    "spellcheck",
    "spellchecker",
    "hunspell",
    "spell check",
    "spell checker",
    "check"
  ],
  "repository": "https://github.com/bjarkebech/nodehun-native",
  "bugs": "https://github.com/bjarkebech/nodehun-native/issues",
  "author": "Bjarke Bech <bjarkebech@gmail.com>",
  "contributors": [
    "Nathan Sweet <nathanjsweet@gmail.com>",
    "Haris Khan <hk12@live.ca>",
    "Titus Wormer <tituswormer@gmail.com>",
    "Espen Hovlandsdal <espen@hovlandsdal.com>",
    "Thomas Beverley <tom.beverley101@gmail.com>",
    "Bjarke Bech <bjarkebech@gmail.com>"
  ],
  "type": "commonjs",
  "main": "index.js",
  "types": "src/Nodehun.d.ts",
  "gypfile": true,
  "dependencies": {
    "node-addon-api": "8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "neostandard": "^0.12.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "node-gyp rebuild -j 8",
    "build-debug": "node-gyp rebuild -j 8 --debug",
    "test": "eslint && node test/index.spec.js",
    "test-watch": "node --test --watch test/index.spec.js"
  }
}
